   "source": [
    "\"\"\"Write a Python program to create a contiguous flattened array. \n",
    "Original array:\n",
    "[[10 20 30] \n",
    "[20 40 50]] \n",
    "New flattened array: \n",
    "[10 20 30 20 40 50]\n",
    "\n",
    "\"\"\"\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "array=[10,20,30]\n",
    "array1=[20,40,50]\n",
    "\n",
    "val=np.append(array,array1)\n",
    "print(sorted(val))\n",
    "print(np.ravel(val))\n"
   ]
  }
